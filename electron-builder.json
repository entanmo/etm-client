    "oneClick": false,
    //是否一键安装
    "allowToChangeInstallationDirectory": true,
    //是否允许更改安装目录
    "perMachine": true,
    "include": ""
  },
  "win": {
    //应用图标
    //    "icon": "app/img/Logo",
    "requestedExecutionLevel": "requireAdministrator"
    //需要管理员权限
  },
  "mac": {//mac打包配置
    //应用图标
    "icon": "build/icon.icns"
  },
  "nodeGypRebuild": false,
  //打包之前是否构建
  "portable": {
    "requestExecutionLevel": "user"
    //安装包执行权限
  },
  "npmArgs": [
  ],
  "npmRebuild": false,
  //是否需要npm 构建
  "electronDownload": {
    //electron 镜像
//    "mirror": "https://registry.npm.taobao.org"
  },
